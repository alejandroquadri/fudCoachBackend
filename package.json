{
  "name": "fud-coach-backend",
  "version": "1.0.0",
  "description": "Fud Coach backend api",
  "main": "./dist/index.js",
  "scripts": {
    "start": "nodemon",
    "build": "tsc --project tsconfig.json",
    "run:build": "NODE_ENV=production node dist/index.js",
    "docker:build": "docker build -t fud-node:dev .",
    "docker:run": "docker run -d --name fud-node --env-file .env -p 3000:3000 fud-node:dev",
    "docker:stop": "docker stop fud-node",
    "docker:remove": "docker rm -f fur-node",
    "docker:clean": "docker system prune -af --volumes",
    "docker:logs": "docker logs -f fud-node",
    "docker:isrunning": "docker ps --format 'table {{.Names}}\t{{.Status}}\t{{.Ports}}'"
  },
  "author": "Alejandro Quadri",
  "license": "ISC",
  "dependencies": {
    "@apple/app-store-server-library": "^1.6.0",
    "agenda": "^5.0.0",
    "axios": "^1.5.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.6.0",
    "cors": "^2.8.5",
    "date-fns": "^3.0.6",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.0.3",
    "expo-server-sdk": "^3.15.0",
    "express": "^4.18.2",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.1",
    "mongodb": "^6.1.0",
    "multer": "^1.4.5-lts.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "serialijse": "^0.3.0",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.6.2",
    "@types/passport": "^1.0.12",
    "@types/passport-jwt": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.22",
    "prettier": "2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack": "^5.83.1",
    "webpack-cli": "^5.1.1",
    "webpack-node-externals": "^3.0.0"
  }
}
